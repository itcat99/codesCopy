{
  "name": "Codes Copy",
  "description": "copy codes",
  "version": "1.0.0",
  "background": {
    "scripts": ["./node_modules/axios/dist/axios.js","background.js"]
  },
    "permissions": [
        "activeTab",
        "contextMenus",
        "notifications",
        "http://*/"
    ],
  "content_scripts": [{
    "matches": ["http://*/*","https://*/*"],
    "css": ["./node_modules/epiceditor/epiceditor/themes/base/epiceditor.css","./content/content.css"],
    "js": ["./node_modules/jquery/dist/jquery.min.js", "./node_modules/epiceditor/epiceditor/js/epiceditor.min.js","./content/content.js"]
  }],
  "browser_action": {
    "name": "copy codes"
  },
  "manifest_version": 2
}