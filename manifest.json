{
  "name": "Codes Copy",
  "description": "copy codes",
  "version": "1.0.0",
  "background": {
    "scripts": ["./node_modules/tesseract.js/dist/tesseract.js","./node_modules/axios/dist/axios.js","background.js"]
  },
    "permissions": [
        "activeTab",
        "tabs",
        "contextMenus",
        "notifications",
        "http://*/*",
        "https://*/*"
    ],
  "content_scripts": [{
    "matches": ["http://*/*","https://*/*"],
    "css": ["./node_modules/epiceditor/epiceditor/themes/base/epiceditor.css","./content/content.css","./node_modules/csspin/css/csspin-round.css"],
    "js": ["./node_modules/jquery/dist/jquery.min.js", "./node_modules/epiceditor/epiceditor/js/epiceditor.min.js","./content/content.js"]
  }],
  "browser_action": {
    "name": "copy codes"
  },
  "web_accessible_resources":[
    "./images/fre.png",
    "./node_modules/tesseract.js/dist/worker.js",
    "./node_modules/tesseract.js-core/index.js"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://cdn.rawgit.com/naptha/; object-src 'self'",
  "manifest_version": 2
}